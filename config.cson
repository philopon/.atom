"*":
  welcome:
    showOnStartup: false
  core:
    packagesWithKeymapsDisabled: [
      "git-plus"
      "git-history"
      "highlight-line"
      "regex-railroad-diagram"
    ]
    disabledPackages: [
      "exception-reporting"
    ]
  editor:
    invisibles: {}
    tabLength: 4
    showIndentGuide: true
    showInvisibles: true
    scrollPastEnd: true
  "ide-haskell":
    onSaveLint: false
    closeTooltipsOnCursorMove: true
  "vim-mode":
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  "autocomplete-python":
    useSnippets: "required"
  "autocomplete-plus":
    confirmCompletion: "tab"
    autoActivationDelay: 0
    backspaceTriggersAutocomplete: true
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
